  rdfs:domain  battery:Performance ;
  rdfs:range   battery:BatteryExtension .

battery:rawMaterials
  a owl:ObjectProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/battery> ;
  rdfs:label   "materials"@en ;
