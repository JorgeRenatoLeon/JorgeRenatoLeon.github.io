battery:massPercentage
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/battery> ;
  rdfs:label   "massPercentage"@en ;
  rdfs:comment "Mass percentage of the element (0.0 to 100.0)."@en ;
  rdfs:domain  battery:MaterialCompositionEntry ;
