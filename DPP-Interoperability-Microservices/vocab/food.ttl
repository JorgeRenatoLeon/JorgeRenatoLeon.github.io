# =============================================================================
# Datatype Properties — Food Safety
# =============================================================================

food:safetyCertification
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/food> ;
  rdfs:label   "safetyCertification"@en ;
  rdfs:comment "Food safety certification (e.g. 'ISO 22000:2018', 'BRC Food Safety', 'IFS Food', 'HACCP')."@en ;
  rdfs:domain  food:FoodSafetyDeclaration ;
  rdfs:range   xsd:string .

food:certificationBody
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/food> ;
  rdfs:label   "certificationBody"@en ;
  rdfs:comment "Organization that issued the food safety certification."@en ;
  rdfs:domain  food:FoodSafetyDeclaration ;
  rdfs:range   xsd:string .

food:certificationDate
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/food> ;
  rdfs:label   "certificationDate"@en ;
  rdfs:comment "Date the certification was issued."@en ;
  rdfs:domain  food:FoodSafetyDeclaration ;
