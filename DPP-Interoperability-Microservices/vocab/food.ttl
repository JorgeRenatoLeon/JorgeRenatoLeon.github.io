food:certificationExpiry
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/food> ;
  rdfs:label   "certificationExpiry"@en ;
  rdfs:comment "Date the certification expires."@en ;
  rdfs:domain  food:FoodSafetyDeclaration ;
  rdfs:range   xsd:date .

food:certificationLink
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/food> ;
  rdfs:label   "certificationLink"@en ;
  rdfs:comment "URL to the certification document."@en ;
  rdfs:domain  food:FoodSafetyDeclaration ;
  rdfs:range   xsd:anyURI .
