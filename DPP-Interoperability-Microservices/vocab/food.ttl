# =============================================================================
# Datatype Properties — Storage Conditions
# FIX: food:expiryType and food:shelfLifeDays moved from food:FoodExtension
# to food:StorageConditions where all physical storage data lives.
# =============================================================================

food:temperatureMin
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/food> ;
  rdfs:label   "temperatureMin"@en ;
  rdfs:comment "Minimum storage temperature (°C)."@en ;
  rdfs:domain  food:StorageConditions ;
  rdfs:range   xsd:double .

food:temperatureMax
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/food> ;
  rdfs:label   "temperatureMax"@en ;
  rdfs:comment "Maximum storage temperature (°C)."@en ;
  rdfs:domain  food:StorageConditions ;
  rdfs:range   xsd:double .

food:humidity
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/food> ;
  rdfs:label   "humidity"@en ;
  rdfs:comment "Required storage humidity range description."@en ;
  rdfs:domain  food:StorageConditions ;
  rdfs:range   xsd:string .

food:specialInstructions
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/food> ;
  rdfs:label   "specialInstructions"@en ;
  rdfs:comment "Special storage instructions (e.g. 'Keep refrigerated', 'Protect from light')."@en ;
  rdfs:domain  food:StorageConditions ;
  rdfs:range   xsd:string .

# FIX: domain corrected from food:FoodExtension → food:StorageConditions
food:expiryType
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/food> ;
  rdfs:label   "expiryType"@en ;
  rdfs:comment "Date marking type: 'use_by' (food safety) or 'best_before' (quality). Per EU FIC Article 24."@en ;
  rdfs:domain  food:StorageConditions ;
  rdfs:range   xsd:string .

# FIX: domain corrected from food:FoodExtension → food:StorageConditions
food:shelfLifeDays
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/food> ;
  rdfs:label   "shelfLifeDays"@en ;
  rdfs:comment "Expected shelf life in days under recommended storage conditions."@en ;
  rdfs:domain  food:StorageConditions ;
  rdfs:range   xsd:integer .

