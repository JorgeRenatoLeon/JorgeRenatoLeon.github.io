lcaef:verificationBody
  a owl:DatatypeProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "verificationBody"@en ;
