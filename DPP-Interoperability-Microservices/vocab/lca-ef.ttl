@prefix lcaef:   <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef#> .
@prefix dpp:     <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/core#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix qudt:    <http://qudt.org/schema/qudt/> .

<https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef>
  a owl:Ontology ;
  owl:versionIRI   <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef/0.2> ;
  owl:versionInfo  "0.2" ;
  owl:imports      <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/dpp> ;
  rdfs:label       "DPP LCA/Environmental Footprint Extension Vocabulary"@en ;
  dcterms:description
    "Lifecycle Assessment and Environmental Footprint extension vocabulary for the DPP Interoperability Platform. Covers carbon footprint, GHG scopes, lifecycle stages, impact categories, circularity metrics, and water footprint per GHG Protocol, ISO 14040, PEF, and CIRPASS frameworks."@en ;
  dcterms:creator  "Jorge Renato Leon Chumpitaz" ;
  dcterms:created  "2026-02-06"^^xsd:date ;
  dcterms:modified "2026-02-23"^^xsd:date ;
  dcterms:license  <https://creativecommons.org/licenses/by/4.0/> ;
  dcterms:rights   "CC BY 4.0 — Jorge Renato Leon Chumpitaz" ;
  dcterms:subject  "Digital Product Passport", "LCA", "Environmental Footprint",
                   "GHG Protocol", "ISO 14040", "PEF" ;
  rdfs:seeAlso     <https://ghgprotocol.org/> ,
                   <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:32013H0179> .

# =============================================================================
# Classes
# =============================================================================

lcaef:LcaEfExtension
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "LCA-EF Extension"@en ;
  rdfs:comment "Container class for lifecycle assessment and environmental footprint data within a DPP."@en .

lcaef:CarbonFootprint
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "Carbon Footprint"@en ;
  rdfs:comment "Carbon footprint including total value, GHG scope breakdown, and lifecycle stage breakdown."@en ;
  rdfs:seeAlso <https://ghgprotocol.org/> .

lcaef:GhgScopes
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "GHG Scopes"@en ;
  rdfs:comment "GHG emissions broken down by GHG Protocol scope (1, 2, 3)."@en ;
  rdfs:seeAlso <https://ghgprotocol.org/corporate-standard> .

lcaef:LifecycleStage
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "Lifecycle Stage"@en ;
  rdfs:comment "Environmental impact for a specific lifecycle stage (raw material extraction, manufacturing, distribution, use phase, end-of-life)."@en ;
  rdfs:seeAlso <https://eur-lex.europa.eu/legal-content/EN/TXT/?uri=CELEX:32013H0179> .

lcaef:ImpactCategory
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "Impact Category"@en ;
  rdfs:comment "LCA impact category result (e.g. Climate Change/GWP, Acidification, Eutrophication)."@en .

lcaef:Methodology
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "LCA Methodology"@en ;
  rdfs:comment "Methodology information for the LCA: standard, system boundary, and cut-off criteria."@en ;
  rdfs:seeAlso <https://www.iso.org/standard/37456.html> .

lcaef:DataQuality
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "Data Quality"@en ;
  rdfs:comment "Data quality metadata: temporal, geographical, and technology coverage, plus primary data percentage."@en .

lcaef:Performer
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "LCA Performer"@en ;
  rdfs:comment "Organization or person that performed the lifecycle assessment."@en .

lcaef:Reviewer
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "LCA Reviewer"@en ;
  rdfs:comment "Organization or person that reviewed the lifecycle assessment."@en .

lcaef:CircularityMetrics
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "Circularity Metrics"@en ;
  rdfs:comment "Product circularity metrics: recyclability, recycled content, repairability, and MCI."@en .

lcaef:WaterFootprint
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "Water Footprint"@en ;
  rdfs:comment "Water footprint broken down by type: blue (surface/ground), green (rain), grey (dilution)."@en .

lcaef:ComplianceDeclaration
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "Compliance Declaration"@en ;
  rdfs:comment "Environmental compliance or regulation declaration."@en .

# NEW CLASS: required as range for lcaef:scope3Categories
lcaef:Scope3Category
  a owl:Class ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "Scope 3 Category"@en ;
  rdfs:comment "A GHG Protocol Scope 3 category (one of the 15 categories in the Corporate Value Chain Standard)."@en ;
  rdfs:seeAlso <https://ghgprotocol.org/scope-3-calculation-guidance-2> .

# Disjointness between top-level sub-classes
lcaef:CarbonFootprint  owl:disjointWith lcaef:CircularityMetrics, lcaef:WaterFootprint,
                                         lcaef:Methodology, lcaef:DataQuality .
lcaef:GhgScopes        owl:disjointWith lcaef:LifecycleStage, lcaef:ImpactCategory .
lcaef:Performer        owl:disjointWith lcaef:Reviewer .

# =============================================================================
# Object Properties
# =============================================================================

lcaef:carbonFootprint
  a owl:ObjectProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label    "carbonFootprint"@en ;
  rdfs:comment  "Links LCA-EF extension to its carbon footprint data."@en ;
  rdfs:domain   lcaef:LcaEfExtension ;
  rdfs:range    lcaef:CarbonFootprint ;
  owl:inverseOf lcaef:isCarbonFootprintOf .

lcaef:isCarbonFootprintOf
  a owl:ObjectProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "isCarbonFootprintOf"@en ;
  rdfs:comment "Inverse of lcaef:carbonFootprint."@en ;
  rdfs:domain  lcaef:CarbonFootprint ;
  rdfs:range   lcaef:LcaEfExtension .

lcaef:byScope
  a owl:ObjectProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "byScope"@en ;
  rdfs:comment "Links carbon footprint to GHG scope breakdown."@en ;
  rdfs:domain  lcaef:CarbonFootprint ;
  rdfs:range   lcaef:GhgScopes .

lcaef:byLifecycleStage
  a owl:ObjectProperty ;
  rdfs:isDefinedBy <https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/lca-ef> ;
  rdfs:label   "byLifecycleStage"@en ;
  rdfs:comment "Links carbon footprint to lifecycle stage breakdown."@en ;
  rdfs:domain  lcaef:CarbonFootprint ;
  rdfs:range   lcaef:LifecycleStage .

# FIX: missing rdfs:range added
lcaef:lifecycleStages
  a owl:ObjectProperty ;
