{
  "@context": {
    "@version": 1.1,
    "@vocab": "https://schema.org/",
    "dpp": "https://jorgerenatoleon.github.io/DPP-Interoperability-Microservices/vocab/dpp#",
    "prov": "http://www.w3.org/ns/prov#",
    "qudt": "http://qudt.org/schema/qudt/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dcterms": "http://purl.org/dc/terms/",
    "passportId": {
      "@id": "dpp:passportId",
      "@type": "@id"
    },
    "version": {
      "@id": "dpp:version",
      "@type": "xsd:string"
    },
    "lastUpdated": {
      "@id": "dpp:lastUpdated",
      "@type": "xsd:dateTime"
    },
    "product": {
      "@id": "dpp:product"
    },
    "category": {
      "@id": "dpp:productCategory",
      "@type": "xsd:string"
    },
    "model": {
      "@id": "dpp:productModel",
      "@type": "xsd:string"
    },
    "description": {
      "@id": "description",
      "@type": "xsd:string"
    },
    "manufacturer": {
      "@id": "manufacturer",
      "@type": "Organization"
    },
    "legalName": {
      "@id": "legalName",
      "@type": "xsd:string"
    },
    "facility": {
      "@id": "dpp:facility"
    },
    "country": {
      "@id": "addressCountry",
      "@type": "xsd:string"
    },
    "manufacturingDate": {
      "@id": "dpp:manufacturingDate",
      "@type": "xsd:date"
    },
    "placingOnMarket": {
      "@id": "dpp:placingOnMarket",
