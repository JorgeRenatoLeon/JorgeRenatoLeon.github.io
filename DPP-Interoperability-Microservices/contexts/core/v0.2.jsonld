    },
    "economicOperator": {
      "@id": "dpp:economicOperator"
    },
    "status": {
      "@id": "dpp:passportStatus",
      "@type": "xsd:string"
    },
    "identifiers": {
      "@id": "dpp:identifiers",
      "@container": "@set"
    },
    "type": {
      "@id": "dpp:identifierType",
      "@type": "xsd:string"
    },
    "value": {
      "@id": "dpp:identifierValue",
      "@type": "xsd:string"
    },
    "granularity": {
      "@id": "dpp:identifierGranularity",
      "@type": "xsd:string"
    },
    "dataCarrier": {
      "@id": "dpp:dataCarrier"
    },
    "url": {
      "@id": "url",
      "@type": "@id"
    },
    "composition": {
      "@id": "dpp:composition"
    },
    "componentMaterials": {
      "@id": "dpp:componentMaterials",
      "@container": "@set"
    },
    "recycledContent": {
      "@id": "dpp:recycledContent",
      "@container": "@set"
    },
    "name": {
      "@id": "name",
      "@type": "xsd:string"
    },
    "mass": {
      "@id": "dpp:mass"
    },
    "percentage": {
      "@id": "dpp:percentage",
      "@type": "xsd:double"
    },
    "material": {
      "@id": "dpp:materialName",
      "@type": "xsd:string"
    },
    "extensions": {
      "@id": "dpp:extensions",
      "@container": "@index"
    },
    "provenance": {
      "@id": "prov:entity"
    },
    "source": {
      "@id": "prov:wasAttributedTo",
      "@type": "xsd:string"
    },
    "timestamp": {
      "@id": "prov:generatedAtTime",
      "@type": "xsd:dateTime"
    },
    "fields": {
      "@id": "dpp:provenanceFields",
      "@container": "@index"
    }
